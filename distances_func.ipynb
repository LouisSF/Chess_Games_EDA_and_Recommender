   "source": [
    "def cos_similarity(a, b):\n",
    "    dot_prod = np.dot(a, b)\n",
    "    norm_a = np.linalg.norm(a)\n",
    "    norm_b = np.linalg.norm(b)\n",
    "    return dot_prod/(norm_a*norm_b)\n"
   ]
  }
